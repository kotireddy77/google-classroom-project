		<!-- Google OAuth2 and HTTP Transport -->
		<dependency>
			<groupId>com.google.oauth-client</groupId>
			<artifactId>google-oauth-client</artifactId>
			<version>1.34.1</version>
		</dependency>



		<dependency>
			<groupId>com.google.api-client</groupId>
			<artifactId>google-api-client</artifactId>
			<version>1.35.0</version> <!-- Make sure to use the correct version -->
		</dependency>

		<!-- Google OAuth2 Library -->
		<dependency>
			<groupId>com.google.auth</groupId>
			<artifactId>google-auth-library-oauth2-http</artifactId>
			<version>0.27.0</version>
		</dependency>

		<!-- Google API HTTP Transport -->
		<dependency>
			<groupId>com.google.http-client</groupId>
			<artifactId>google-http-client-gson</artifactId>
			<version>1.40.1</version> <!-- Make sure to use the correct version -->
		</dependency>

		<!-- Google API JSON Library -->
		<dependency>
			<groupId>com.google.http-client</groupId>
			<artifactId>google-http-client</artifactId>
			<version>1.40.1</version> <!-- Make sure to use the correct version -->
		</dependency>

		<!-- Google Sheets API -->









	</dependencies>

	<build>
		<plugins>
			<!-- Enable preview features during compilation -->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.13.0</version> <!-- Make sure this supports Java 23 -->
				<configuration>
					<release>23</release>
					<compilerArgs>
						<arg>--enable-preview</arg>
					</compilerArgs>
				</configuration>
			</plugin>

			<!-- Spring Boot plugin with preview feature enabled at runtime -->
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
				<configuration>
					<jvmArguments>--enable-preview</jvmArguments>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>


























